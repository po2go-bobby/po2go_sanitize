{
  "name": "po2go/po2go_sanitize",
  "description": "Created with composer init",
  "type": "library",
  "require": {
    "php": "^5.6 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "3.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Bobby Pearson",
      "email": "bobby@punchout2go.com"
    }
  ],
  "keywords": [
    "punchout2go",
    "sanitize",
    "tutorial"
  ],
  "homepage": "https://public.punchout2go.com/common/po2go_sanitize.html",
  "autoload": {
    "psr-4": {
      "po2go\\po2go_sanitize\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "po2go\\po2go_sanitize\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpos -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  }
}